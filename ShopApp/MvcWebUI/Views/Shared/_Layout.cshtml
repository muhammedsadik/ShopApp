

<!DOCTYPE html>

<html>

<head>

  <link rel="stylesheet" href="/wwwroot/lib/bootstrap/dist/css/bootstrap.css" />
  @RenderSection("Css",false)

  <title>ShopApp</title>
</head>

<body>

  @await Html.PartialAsync("_navbar")



  <main class="my-3">
    <div class="container">

      @if (@TempData["message"] != null)
      {
        <partial name="_ResultMessage" model="@(TempData.Get<AlertMessage>("message"))" />
      }

      @RenderBody()

    </div>
  </main>

  <footer class="py-5 mt-3 bg-dark text-white text-center sticky-md-bottom">
    Shop App | 2025
  </footer>



  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  @RenderSection("Scripts",false)

</body>
</html>
